<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.map.unlimited.easy.tool">

    <application
        android:name="com.map.unlimited.easy.tool.Map"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        tools:replace="android:theme"
        android:theme="@style/Theme.Map">
        <activity
            android:name="com.map.unlimited.easy.tool.ac.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.map.unlimited.easy.tool.ac.HomeActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.map.unlimited.easy.tool.ac.ChooseServerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.map.unlimited.easy.tool.ac.ConnectResultActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.map.unlimited.easy.tool.ac.WebActivity"
            android:screenOrientation="portrait"/>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_id" />
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
    </application>

</manifest>