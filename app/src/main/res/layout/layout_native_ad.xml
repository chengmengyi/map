<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="243dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.google.android.gms.ads.nativead.NativeAdView
        android:id="@+id/nv_root_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_ffffff_10dp_solid">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.google.android.gms.ads.nativead.MediaView
                android:id="@+id/mv_native_cover"
                android:layout_width="match_parent"
                android:layout_height="188dp"
                app:layout_constraintTop_toTopOf="parent"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_tag_left"
                android:layout_width="23dp"
                android:layout_height="14dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:src="@drawable/icon_home_native_ad_tag"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_tag_right"
                android:layout_width="23dp"
                android:layout_height="14dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:src="@drawable/icon_result_native_ad_tag"/>
            <androidx.constraintlayout.utils.widget.ImageFilterView
                android:id="@+id/iv_native_logo"
                android:layout_width="36dp"
                android:layout_height="36dp"
                app:round="4dp"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="11dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_native_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:ellipsize="end"
                android:singleLine="true"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:text="High School Escape 2"
                android:textColor="@color/color_333333"
                app:layout_constraintTop_toTopOf="@id/iv_native_logo"
                app:layout_constraintLeft_toRightOf="@id/iv_native_logo"
                app:layout_constraintRight_toLeftOf="@id/tv_native_action"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_native_desc"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:ellipsize="end"
                android:singleLine="true"
                android:textColor="#797979"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:text="Press the spacebar andâ€¦"
                app:layout_constraintLeft_toRightOf="@id/iv_native_logo"
                app:layout_constraintTop_toBottomOf="@id/tv_native_title"
                app:layout_constraintRight_toLeftOf="@id/tv_native_action"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_native_action"
                android:layout_width="88dp"
                android:layout_height="36dp"
                android:gravity="center"
                android:textSize="14sp"
                android:text="install"
                android:layout_margin="10dp"
                android:textColor="@color/white"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/native_ad_action_selector"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.gms.ads.nativead.NativeAdView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/iv_native_default"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:src="@drawable/bg_native_ad_default"/>
</RelativeLayout>